<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tasks List</title>
</head>
<body>
    <h1>Tasks</h1>
    <ul>
        <% tasks.forEach(function(task) { %>
            <li><%= task.description %> - <%= task.completed ? 'Completed' : 'Not Completed' %></li>
        <% }) %>
    </ul>
</body>
</html>
